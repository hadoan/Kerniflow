{
  "name": "@kerniflow/domain",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "dependencies": {
    "@kerniflow/contracts": "workspace:*"
  },
  "scripts": {
    "build": "pnpm -w exec tsdown",
    "dev": "pnpm -w exec tsdown --watch",
    "typecheck": "pnpm -w exec tsc -p tsconfig.json --noEmit"
  }
}
