model Expense {
  id              String   @id @default(cuid())
  tenantId        String
  merchant        String
  totalCents      Int
  currency        String
  category        String?
  issuedAt        DateTime @db.Timestamptz(6)
  custom          Json?
  createdByUserId String
  createdAt       DateTime @default(now()) @db.Timestamptz(6)
  updatedAt       DateTime @updatedAt @db.Timestamptz(6)

  @@index([tenantId])
  @@index([tenantId, issuedAt])
}
